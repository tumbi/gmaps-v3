<%#*<div class = "formsignin">%>
<%#*<h2>Sign in</h2>%>

<%#= simple_form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>
<%#*<div class="inputs">%>
<%#= f.input :email, :required => false, :autofocus => true %>
<%#= f.input :password, :required => false %>
<%#= f.input :remember_me, :as => :boolean if devise_mapping.rememberable? %>
<%#*</div>%>

<%#*<div class="actions">%>
<%#= f.button :submit, "Sign in" %>
<%#*</div>%>
<%# end %>

<%#= render "links" %>
<%#*</div>%>

<div class="clearfix"></div>
<div class="mainbanner">
  <div class="container">
    <div class="row-fluid">
      <div class="span12">
        <div class="slogancap"></div>
        <div class="slogan">Modern way of managing rental business
          and tracking your resources
          <br />
          <small>Free 14-day trial with 24/7 support. No credit card required.</small>
          <br />
          <button class="btn btn-banner">Get Started</button>

          <button class="btn btn-banner transp">Watch Demo <i class="icon-caret-right icon-large"></i></button>
        </div>
      </div>
    </div>
  </div>
</div>